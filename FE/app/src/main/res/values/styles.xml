<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--
        Theme.FEApp 테마를 MaterialComponents 기반으로 정의
        (이름은 프로젝트 상황에 맞게 바꿔도 됩니다.)
    -->
    <style name="Theme.FEApp" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary / Secondary 색상(예시) -->
        <item name="colorPrimary">@color/teal_700</item>
        <item name="colorPrimaryVariant">@color/teal_800</item>
        <item name="colorOnPrimary">@color/white</item>

        <item name="colorSecondary">@color/purple_500</item>
        <item name="colorOnSecondary">@color/white</item>

        <!-- 여기서 반드시 배경색 속성을 정의해야 ?attr/colorBackground 참조가 가능 -->
        <item name="android:colorBackground">@color/white</item>
    </style>

</resources>
